datasource mysql {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Producto{
  id Int @id @default(autoincrement())
  nombre String
  precio Float
  // Add other fields as needed
}

model Usuario{
  id Int @id @default(autoincrement())
  nombre String
  direccion String

}
